    <submodel-path alias="../../rocket/submodel-path"/>
    <x-offset>  6.5277135 </x-offset> <!-- -1.98955 m =  6.5277135 ft -->
    <y-offset>  6.6156115 </y-offset> <!-- -2.01634 m =  6.6156115 ft -->
    <z-offset> -4.4864722 </z-offset> <!-- -1.36741 m = -4.4864722 ft -->
