<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <cannon>
    <name>Guns</name>
    <model>Aircraft/O-2A/Models/Effects/guns/tracer.xml</model>
    <trigger>controls/armament/trigger-cannon</trigger>
    <speed>2755.9</speed>
    <repeat>true</repeat>
    <delay>0.41</delay>
    <count>-1</count>
    <yaw-offset>0</yaw-offset>
    <pitch-offset>0.1</pitch-offset>
    <life>20</life>
    <wind>false</wind>
    <weight>0.13007235</weight>   
    <cd>0.193</cd>
    <eda>0.00571198</eda>
    <fuse-range>0</fuse-range>
    <collision>true</collision>
    <collision-report>sim/ai/aircraft/collision/gun</collision-report>
    <expiry>true</expiry>
    <impact>true</impact>
    <impact-report>sim/ai/aircraft/impact/bullet</impact-report>
    <submodel-path>Aircraft/O-2A/Models/Effects/guns/bullet-submodel.xml</submodel-path>
  </cannon>

  <rocket>
    <model>Aircraft/O-2A/Models/weapons/rocket-smoke.xml</model>
    <trigger>controls/armament/trigger-rocket</trigger>
    <speed>2000</speed>                         <!-- ft/sec -->
    <repeat>true</repeat>
    <delay>0.05</delay>                          <!-- 0.05 -->
    <count>-1</count>                           <!-- 7 rockets per pod  -->
    <yaw-offset>0</yaw-offset>
    <pitch-offset>0</pitch-offset>
    <life>120</life>
    <wind>false</wind>
    <weight>5</weight>                         <!-- lbs. (WP rockets) -->
    <cd>0.075</cd>
    <eda>0.01051198</eda>
    <collision>true</collision>
    <collision-report>sim/ai/aircraft/collision/gun</collision-report>
    <impact>true</impact>
    <impact-report>sim/ai/aircraft/impact/bullet</impact-report>
    <submodel-path>Aircraft/O-2A/Models/Effects/Rocket/Rocket-Submodel.xml</submodel-path>
  </rocket>
  
  <!-- Rockets -->
 
  
  <submodel>
    <name>Left interior Rockets</name>
    <model alias="../../rocket/model"/>
